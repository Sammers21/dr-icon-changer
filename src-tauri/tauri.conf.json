{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "DR Icon Changer",
  "version": "1.0.0",
  "identifier": "io.github.sammers21.dr-icon-changer",
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/sammers21/dr-icon-changer/releases/latest/download/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDQ4NjlCOTlBQTY3OTlDQzcKUldUSG5IbW1tcmxwU0V3YkhOWWtmblpTL1NEY2p4dm5PemNuTWpJK1Jjams4TUNNazJmRzlpZ00K"
    }
  },
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
